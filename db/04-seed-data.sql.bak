-- ============================================
-- 在庫データの初期化
-- ============================================
-- 簡略化のため、全商品一律で在庫を登録（実際の運用では個別に設定）
INSERT INTO product_stocks (product_id, quantity)
SELECT id, 
    CASE 
        WHEN id % 5 = 0 THEN 0  -- 5件に1件は在庫切れ
        WHEN id % 3 = 0 THEN 10 -- 少なめ
        ELSE 100                -- 通常在庫
    END
FROM products
ON CONFLICT (product_id) DO NOTHING;
